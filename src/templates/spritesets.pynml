<tal:spritesets repeat="spriteset sprite_manager.values()">
    spriteset(${spriteset.id}) {
        <tal:sprites repeat="sprite spriteset">
            ${sprite.id}: [${sprite.x_loc}, ${sprite.y_loc}, ${sprite.width}, ${sprite.height}, ${sprite.x_offset}, ${sprite.y_offset}, ANIM | NOCROP, "${sprite.graphics_file_path}"]
        </tal:sprites>
    }
</tal:spritesets>
