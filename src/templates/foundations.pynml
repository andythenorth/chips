        STORE_TEMP(switch_initialise_foundations_objects_slope_se_nw(), ${graphics_temp_storage.var_hide_foundation_water_and_coast_slope_se_nw}),
        STORE_TEMP(switch_initialise_foundations_objects_slope_sw_ne(), ${graphics_temp_storage.var_hide_foundation_water_and_coast_slope_sw_ne}),
